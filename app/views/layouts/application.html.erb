<!DOCTYPE html>
<html>
<head>
  <title>Stumblr</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if signed_in? %>
  <p>Logged in as <%= current_user.username %></p>
  <%= button_to "Log Out", api_sessions_url, method: :delete %>
<% end %>

<%= yield %>

</body>
</html>
